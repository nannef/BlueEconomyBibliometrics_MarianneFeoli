	keywords = {Coastal zones; Economic and social effects; Economics; Marine biology; Planning; Regional planning; Absorptive capacity; Blue economies; Blue growth; Coastal regions; Environmental forces; Marine resources; Marine sector; Regional economic development; Sustainable blue economy; Triple helixes; Sustainable development},
